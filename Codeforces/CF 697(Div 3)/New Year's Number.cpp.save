#include<bits/stdc++.h>

using namespace std;

typedef long long ll;

int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n,flag=0;
        cin>>n;
        if(n%2020==0) flag=1;
        else if(n%2021==0) flag=1;
        else if(n%4041==0) flag=1;
        else if(n%4040==0) flag=1;
        else if(n%4042==0) flag=1;
        else if(n%6060==0) flag=1;
        else if(n%6063==0) flag=1;
        else if(n%60==0) flag=1;
        if(flag) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
    return 0;
}

